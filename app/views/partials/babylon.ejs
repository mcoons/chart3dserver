<script>

    var socket = io();

    socket.on("connect", function(){
        socket.emit('msg','message from client');
    });

    var el = document.getElementById('server-time');
    var el2 = document.getElementById('client-time');

    socket.on('time', function(svrMsg){
        el.innerText = 'Server time: ' + svrMsg;
        el2.innerText = 'Client time: ' + new Date().toTimeString();

    });


    socket.on('addChartOrder', (chart, options) => {
        console.log('addChartOrder');
        console.log(chart);
        console.log(options);
    });
    socket.on('updateSceneOrder', (scene, options) => {
        console.log('updateSceneOrder');
        console.log(scene);
        console.log(options);
    });
    socket.on('updateChartOrder', (chart, options) => {
        console.log('updateChartOrder');
        console.log(chart);
        console.log(options);
    });
    socket.on('removeChartOrder', (chart, options) => {
        console.log('removeChartOrder');
        console.log(chart);
        console.log(options);
    });
    socket.on('APIreply', (options) => { 
        console.log('APIreply');
        console.log(options);
    });

</script>